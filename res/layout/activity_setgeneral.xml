                android:src="@drawable/separator" />

            <ImageView
                android:id="@+id/setgeneral_bottom4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:src="@drawable/setgeneral_bottom4" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:src="@drawable/separator" />

            <ImageView
                android:layout_width="match_parent"
